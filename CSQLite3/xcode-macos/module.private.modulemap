// macOS System Module map for SQLite3

module CSQLite3 [system] {
  
  header "/Applications/Xcode.app/Contents/Developer/Platforms/MacOSX.platform/Developer/SDKs/MacOSX.sdk/usr/include/sqlite3.h"
  
  export *
  
  link "sqlite3"
}
